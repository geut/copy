{
  "name": "copy-asset",
  "version": "1.0.0",
  "description": "An async lib to copy assets to a custom destination and return a filemeta with the new path.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "pretest": "del-cli test/dest",
    "test": "ava test --verbose",
    "posttest": "eslint .",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "del-cli dist && babel src --out-dir dist",
    "start": "del-cli dist && babel src --watch --source-maps --out-dir dist",
    "prepublish": "npm run test && npm run build"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "browserify": "^13.0.1",
    "coveralls": "^2.11.9",
    "del-cli": "^0.2.0",
    "eslint": "^2.11.1",
    "eslint-config-postcss": "^2.0.2",
    "eslint-config-tinchoz49": "^2.1.0",
    "nyc": "^6.4.4"
  },
  "eslintConfig": {
    "extends": "tinchoz49",
    "rules": {
      "max-len": 0
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "add-module-exports",
      "transform-object-assign"
    ]
  },
  "ava": {
    "require": "babel-register"
  },
  "nyc": {
    "exclude": [
      "src/__tests__/**"
    ]
  },
  "keywords": [
    "browserify",
    "plugin",
    "copy",
    "assets"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geut/copy-asset.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geut/copy-asset/issues"
  },
  "homepage": "https://github.com/geut/copy-asset#readme",
  "dependencies": {
    "mkdirp": "^0.5.1",
    "pify": "^2.3.0",
    "read-pkg-up": "^1.0.1",
    "stack-trace": "0.0.9"
  }
}
